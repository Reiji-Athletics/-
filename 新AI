    async function analyzeForm() {
        const imageInput = document.getElementById('imageInput');
        const canvas = document.getElementById('poseCanvas');
        const ctx = canvas.getContext('2d');
        const adviceDiv = document.getElementById('advice');

        if (!imageInput.files[0]) {
            showMessage('ç”»åƒã‚’ã‚¢ãƒƒãƒ—ãƒ­ãƒ¼ãƒ‰ã—ã¦ãã ã•ã„ï¼', 'error');
            return;
        }

        // ãƒ­ãƒ¼ãƒ‡ã‚£ãƒ³ã‚°è¡¨ç¤º
        adviceDiv.innerHTML = '<div class="loading"></div>ç”»åƒã‚’åˆ†æä¸­...';

        const img = new Image();
        img.src = URL.createObjectURL(imageInput.files[0]);
        
        img.onload = () => {
            canvas.width = Math.min(img.width, 600);
            canvas.height = (img.height * canvas.width) / img.width;
            ctx.drawImage(img, 0, 0, canvas.width, canvas.height);
            canvas.style.display = 'block';
            
            // ã‚·ãƒŸãƒ¥ãƒ¬ãƒ¼ãƒˆã•ã‚ŒãŸãƒ•ã‚©ãƒ¼ãƒ åˆ†æ
            setTimeout(() => {
                simulateFormAnalysis();
            }, 2000);
        };
    }

    function simulateFormAnalysis() {
        const adviceDiv = document.getElementById('advice');
        const canvas = document.getElementById('poseCanvas');
        const ctx = canvas.getContext('2d');
        
        // ãƒ•ã‚©ãƒ¼ãƒ åˆ†æã®ã‚·ãƒŸãƒ¥ãƒ¬ãƒ¼ã‚·ãƒ§ãƒ³
        const kneeScore = Math.random() * 40 + 60; // 60-100
        const armScore = Math.random() * 40 + 60;  // 60-100
        currentFormScore = Math.floor((kneeScore + armScore) / 2);
        
        // ãƒãƒ¼ã‚ºã®æç”»ã‚’ã‚·ãƒŸãƒ¥ãƒ¬ãƒ¼ãƒˆ
        drawSimulatedPose(ctx);
        
        let advice = '<h3>ğŸƒâ€â™‚ï¸ ãƒ•ã‚©ãƒ¼ãƒ åˆ†æçµæœ</h3>';
        advice += `<div style="background: linear-gradient(45deg, #667eea, #764ba2); color: white; padding: 15px; border-radius: 10px; margin: 10px 0;">`;
        advice += `<h4>ç·åˆã‚¹ã‚³ã‚¢: ${currentFormScore}/100</h4>`;
        advice += `</div>`;
        
        if (kneeScore < 75) {
            advice += '<div style="background: #fff3cd; padding: 10px; border-radius: 8px; margin: 10px 0; border-left: 4px solid #ffc107;">';
            advice += '<strong>ğŸ¦µ è†ã®å‹•ãã«ã¤ã„ã¦ï¼š</strong><br>';
            advice += 'è†ã®ä¸Šã’æ–¹ã‚’ã‚‚ã£ã¨æ„è­˜ã—ã¾ã—ã‚‡ã†ï¼å¤§ãããƒ€ã‚¤ãƒŠãƒŸãƒƒã‚¯ã«ï¼<br>';
            advice += '<strong>ğŸ’ª ãŠã™ã™ã‚ãƒˆãƒ¬ãƒ¼ãƒ‹ãƒ³ã‚°ï¼š</strong> é«˜è†èµ°ï¼ˆ20m Ã— 5ã‚»ãƒƒãƒˆï¼‰';
            advice += '</div>';
        } else {
            advice += '<div style="background: #d4edda; padding: 10px; border-radius: 8px; margin: 10px 0; border-left: 4px solid #28a745;">';
            advice += '<strong>ğŸ¦µ è†ã®å‹•ãï¼š</strong> ã¨ã¦ã‚‚è‰¯ã„ãƒ•ã‚©ãƒ¼ãƒ ã§ã™ï¼ã“ã®èª¿å­ã‚’ã‚­ãƒ¼ãƒ—ï¼';
            advice += '</div>';
        }
        
        if (armScore < 75) {
            advice += '<div style="background: #fff3cd; padding: 10px; border-radius: 8px; margin: 10px 0; border-left: 4px solid #ffc107;">';
            advice += '<strong>ğŸ’ª è…•ã®æŒ¯ã‚Šã«ã¤ã„ã¦ï¼š</strong><br>';
            advice += 'è…•ã‚’ã‚‚ã£ã¨åŠ›å¼·ãæŒ¯ã£ã¦ãƒªã‚ºãƒ ã‚’ä½œã‚Šã¾ã—ã‚‡ã†ï¼<br>';
            advice += '<strong>ğŸ’ª ãŠã™ã™ã‚ãƒˆãƒ¬ãƒ¼ãƒ‹ãƒ³ã‚°ï¼š</strong> è…•æŒ¯ã‚Šãƒ‰ãƒªãƒ«ï¼ˆ30ç§’ Ã— 3ã‚»ãƒƒãƒˆï¼‰';
            advice += '</div>';
        } else {
            advice += '<div style="background: #d4edda; padding: 10px; border-radius: 8px; margin: 10px 0; border-left: 4px solid #28a745;">';
            advice += '<strong>ğŸ’ª è…•ã®æŒ¯ã‚Šï¼š</strong> ç´ æ™´ã‚‰ã—ã„ãƒªã‚ºãƒ ã§ã™ï¼ç¶™ç¶šã—ã¦ãã ã•ã„ï¼';
            advice += '</div>';
        }
        
        advice += '<div style="background: linear-gradient(45deg, #a8edea, #fed6e3); padding: 15px; border-radius: 10px; margin-top: 15px;">';
        advice += '<strong>ğŸ¯ æ¬¡ã®ã‚¹ãƒ†ãƒƒãƒ—ï¼š</strong><br>';
        advice += 'å®šæœŸçš„ã«ç·´ç¿’ã‚’ç¶šã‘ã¦ã€ãƒ•ã‚©ãƒ¼ãƒ ã¨è¨˜éŒ²ã®ä¸¡æ–¹ã‚’å‘ä¸Šã•ã›ã¾ã—ã‚‡ã†ï¼<br>';
        advice += 'æ¯é€±ã®è¨˜éŒ²ã‚’æ¯”è¼ƒã—ã¦æˆé•·ã‚’å®Ÿæ„Ÿã—ã¦ãã ã•ã„ï¼';
        advice += '</div>';
        
        adviceDiv.innerHTML = advice;
    }

    function drawSimulatedPose(ctx) {
        const width = ctx.canvas.width;
        const height = ctx.canvas.height;
        
        // ç°¡å˜ãªã‚¹ãƒ†ã‚£ãƒƒã‚¯ãƒ•ã‚£ã‚®ãƒ¥ã‚¢ã‚’æç”»
        ctx.strokeStyle = '#667eea';
        ctx.lineWidth = 6;
        ctx.lineCap = 'round';
        
        // ä½“ã®ä¸­å¿ƒç‚¹ã‚’è¨ˆç®—
        const centerX = width * 0.5;
        const centerY = height * 0.4;
        
        // é ­
        ctx.beginPath();
        ctx.arc(centerX, centerY - 40, 20, 0, 2 * Math.PI);
        ctx.stroke();
        
        // ä½“
        ctx.beginPath();
        ctx.moveTo(centerX, centerY - 20);
        ctx.lineTo(centerX, centerY + 60);
        ctx.stroke();
        
        // è…•
        ctx.beginPath();
        ctx.moveTo(centerX, centerY);
        ctx.lineTo(centerX - 40, centerY - 20);
        ctx.moveTo(centerX, centerY);
        ctx.lineTo(centerX + 30, centerY + 20);
        ctx.stroke();
        
        // è„š
        ctx.beginPath();
        ctx.moveTo(centerX, centerY + 60);
        ctx.lineTo(centerX - 30, centerY + 120);
        ctx.moveTo(centerX, centerY + 60);
        ctx.lineTo(centerX + 40, centerY + 100);
        ctx.stroke();
        
        // åˆ†æãƒã‚¤ãƒ³ãƒˆã‚’ãƒã‚¤ãƒ©ã‚¤ãƒˆ
        ctx.strokeStyle = '#ff6b6b';
        ctx.lineWidth = 3;
        ctx.setLineDash([5, 5]);
        
        // è†ã®è§’åº¦ã‚’ç¤ºã™å††
        ctx.beginPath();
        ctx.arc(centerX - 15, centerY + 90, 15, 0, 2 * Math.PI);
        ctx.stroke();
        
        // è‚˜ã®è§’åº¦ã‚’ç¤ºã™å††
        ctx.beginPath();
        ctx.arc(centerX - 20, centerY - 10, 12, 0, 2 * Math.PI);
        ctx.stroke();
        
        ctx.setLineDash([]);
    }

    function saveRecord() {
        const name = document.getElementById('nameInput').value.trim();
        const grade = document.getElementById('gradeInput').value.trim();
        const time = parseFloat(document.getElementById('timeInput').value);
        
        if (!name || !grade || !time || time <= 0) {
            showMessage('åå‰ã€å­¦å¹´ã€ã‚¿ã‚¤ãƒ ã‚’ã™ã¹ã¦å…¥åŠ›ã—ã¦ãã ã•ã„ï¼', 'error');
            return;
        }

        const record = {
            id: Date.now(),
            name,
            grade,
            time,
            formScore: currentFormScore || Math.floor(Math.random() * 20 + 80),
            date: new Date().toLocaleDateString('ja-JP', {
                year: 'numeric',
                month: 'short',
                day: 'numeric'
            })
        };
        
        records.push(record);
        showMessage(`${name}ã•ã‚“ã®è¨˜éŒ²ã‚’ä¿å­˜ã—ã¾ã—ãŸï¼`, 'success');
        updateGraph();
        
        // ãƒ•ã‚©ãƒ¼ãƒ ã‚’ãƒªã‚»ãƒƒãƒˆ
        document.getElementById('nameInput').value = '';
        document.getElementById('gradeInput').value = '';
        document.getElementById('timeInput').value = '';
        document.getElementById('imageInput').value = '';
        document.querySelector('.file-input-label').textContent = 'ğŸ“¸ ç”»åƒã‚’ã‚¢ãƒƒãƒ—ãƒ­ãƒ¼ãƒ‰';
    }

    function showMessage(message, type) {
        const messageDiv = document.createElement('div');
        messageDiv.className = type;
        messageDiv.textContent = message;
        document.querySelector('.container').insertBefore(
            messageDiv, 
            document.querySelector('.container').firstChild
        );
        
        setTimeout(() => {
            messageDiv.remove();
        }, 3000);
    }

    let chart;
    let formChart;
    
    function createFormScoreChart() {
        if (records.length === 0) return;
        
        const labels = records.map(r => r.date);
        const formScores = records.map(r => r.formScore);
        
        const ctx = document.getElementById('formScoreCanvas').getContext('2d');
        if (formChart) formChart.destroy();
        
        formChart = new Chart(ctx, {
            type: 'doughnut',
            data: {
                labels: records.map(r => `${r.name} (${r.date})`),
                datasets: [{
                    label: 'ãƒ•ã‚©ãƒ¼ãƒ ã‚¹ã‚³ã‚¢',
                    data: formScores,
                    backgroundColor: [
                        'rgba(255, 99, 132, 0.8)',
                        'rgba(54, 162, 235, 0.8)',
                        'rgba(255, 205, 86, 0.8)',
                        'rgba(75, 192, 192, 0.8)',
                        'rgba(153, 102, 255, 0.8)',
                        'rgba(255, 159, 64, 0.8)'
                    ],
                    borderColor: [
                        'rgba(255, 99, 132, 1)',
                        'rgba(54, 162, 235, 1)',
                        'rgba(255, 205, 86, 1)',
                        'rgba(75, 192, 192, 1)',
                        'rgba(153, 102, 255, 1)',
                        'rgba(255, 159, 64, 1)'
                    ],
                    borderWidth: 3,
                    hoverOffset: 10
                }]
            },
            options: {
                responsive: true,
                plugins: {
                    title: {
                        display: true,
                        text: 'ğŸ¯ ãƒ•ã‚©ãƒ¼ãƒ ã‚¹ã‚³ã‚¢åˆ†æ',
                        font: {
                            size: 20,
                            weight: 'bold'
                        },
                        padding: 20,
                        color: '#333'
                    },
                    legend: {
                        position: 'bottom',
                        labels: {
                            font: {
                                size: 12,
                                weight: 'bold'
                            },
                            color: '#333',
                            padding: 15,
                            usePointStyle: true,
                            pointStyle: 'circle'
                        }
                    },
                    tooltip: {
                        backgroundColor: 'rgba(0,0,0,0.8)',
                        titleColor: '#fff',
                        bodyColor: '#fff',
                        cornerRadius: 10,
                        callbacks: {
                            label: function(context) {
                                const record = records[context.dataIndex];
                                return [
                                    `ãƒ•ã‚©ãƒ¼ãƒ ã‚¹ã‚³ã‚¢: ${context.parsed}ç‚¹`,
                                    `100mã‚¿ã‚¤ãƒ : ${record.time}ç§’`,
                                    `å­¦å¹´: ${record.grade}`
                                ];
                            }
                        }
                    }
                },
                animation: {
                    animateRotate: true,
                    duration: 2000,
                    easing: 'easeOutBounce'
                }
            }
        });
    }
    function updateGraph() {
        if (records.length === 0) return;
        
        const labels = records.map(r => r.date);
        const times = records.map(r => r.time);
        const formScores = records.map(r => r.formScore);
        const nationalAvg = records.map(() => 14.5); // ä»®ã®å…¨å›½å¹³å‡ã‚¿ã‚¤ãƒ 

        const ctx = document.getElementById('graphCanvas').getContext('2d');
        if (chart) chart.destroy();
        
        chart = new Chart(ctx, {
            type: 'bar',
            data: {
                labels,
                datasets: [
                    {
                        label: 'â±ï¸ 100mã‚¿ã‚¤ãƒ ï¼ˆç§’ï¼‰',
                        data: times,
                        backgroundColor: [
                            'rgba(255, 99, 132, 0.8)',
                            'rgba(54, 162, 235, 0.8)',
                            'rgba(255, 205, 86, 0.8)',
                            'rgba(75, 192, 192, 0.8)',
                            'rgba(153, 102, 255, 0.8)',
                            'rgba(255, 159, 64, 0.8)'
                        ],
                        borderColor: [
                            'rgba(255, 99, 132, 1)',
                            'rgba(54, 162, 235, 1)',
                            'rgba(255, 205, 86, 1)',
                            'rgba(75, 192, 192, 1)',
                            'rgba(153, 102, 255, 1)',
                            'rgba(255, 159, 64, 1)'
                        ],
                        borderWidth: 2,
                        borderRadius: 8,
                        borderSkipped: false,
                    }
                ]
            },
            options: {
                responsive: true,
                plugins: {
                    title: {
                        display: true,
                        text: 'ğŸ“Š 100mèµ°ã‚¿ã‚¤ãƒ è¨˜éŒ²',
                        font: {
                            size: 20,
                            weight: 'bold'
                        },
                        padding: 20,
                        color: '#333'
                    },
                    legend: {
                        display: true,
                        position: 'top',
                        labels: {
                            font: {
                                size: 14,
                                weight: 'bold'
                            },
                            color: '#333',
                            padding: 20
                        }
                    },
                    tooltip: {
                        backgroundColor: 'rgba(0,0,0,0.8)',
                        titleColor: '#fff',
                        bodyColor: '#fff',
                        cornerRadius: 10,
                        displayColors: true,
                        callbacks: {
                            label: function(context) {
                                const record = records[context.dataIndex];
                                return [
                                    `ã‚¿ã‚¤ãƒ : ${context.parsed.y}ç§’`,
                                    `åå‰: ${record.name}`,
                                    `å­¦å¹´: ${record.grade}`,
                                    `ãƒ•ã‚©ãƒ¼ãƒ ã‚¹ã‚³ã‚¢: ${record.formScore}ç‚¹`
                                ];
                            }
                        }
                    }
                },
                scales: {
                    y: {
                        beginAtZero: false,
                        min: Math.min(...times) - 1,
                        max: Math.max(...times) + 1,
                        title: {
                            display: true,
                            text: 'ã‚¿ã‚¤ãƒ ï¼ˆç§’ï¼‰',
                            font: {
                                size: 14,
                                weight: 'bold'
                            },
                            color: '#333'
                        },
                        grid: {
                            color: 'rgba(0,0,0,0.1)',
                            borderWidth: 2
                        },
                        ticks: {
                            font: {
                                size: 12,
                                weight: 'bold'
                            },
                            color: '#333',
                            callback: function(value) {
                                return value.toFixed(1) + 'ç§’';
                            }
                        }
                    },
                    x: {
                        title: {
                            display: true,
                            text: 'è¨˜éŒ²æ—¥',
                            font: {
                                size: 14,
                                weight: 'bold'
                            },
                            color: '#333'
                        },
                        grid: {
                            display: false
                        },
                        ticks: {
                            font: {
                                size: 12,
                                weight: 'bold'
                            },
                            color: '#333'
                        }
                    }
                },
                animation: {
                    duration: 1500,
                    easing: 'easeOutBounce'
                }
            }
        });
        
        // ãƒ•ã‚©ãƒ¼ãƒ ã‚¹ã‚³ã‚¢ã®åˆ¥ã‚°ãƒ©ãƒ•ã‚’è¿½åŠ 
        setTimeout(() => {
            createFormScoreChart();
        }, 100);
    }

    // åˆæœŸåŒ–
    document.addEventListener('DOMContentLoaded', function() {
        // ã‚µãƒ³ãƒ—ãƒ«ãƒ‡ãƒ¼ã‚¿ã‚’è¿½åŠ ï¼ˆãƒ‡ãƒ¢ç”¨ï¼‰
        const sampleData = [
